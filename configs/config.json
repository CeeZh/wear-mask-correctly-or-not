{
    "exp_base": "experiments",
    "exp_name": "mobilenet_v3_small",
    "cuda": false,
    "device": "cpu",
    "data": {
        "image_size": [224, 224],
        "flip_prob": 0.5,
        "pixel_mean": [0.485, 0.456, 0.406],
        "pixel_std": [0.229, 0.224, 0.225],
        "train_path": "train_anno",
        "val_path":  "validation_anno",
        "test_path": "test_anno"
    },
    "data_loader": {
        "num_workers": 8
    },
    "model": {
        "backbone": "resnet-18",
        "checkpoint_path": ""
    },
    "solver": {
        "optimizer": "Adam",
        "batch_size": 128,
        "learning_rate": 1e-3,
        "weight_decay": 1e-4,
        "num_epochs": 20,
        "checkpoint_period": 10,
        "log_period": 1,
        "eval_period": 10
    },
    "test": {
        "batch_size": 128,
        "num_workers": 8
    }
}